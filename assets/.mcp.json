{
  "mcpServers": {
    "playwright-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ],
      "env": {}
    },
    "browser-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@cloudflare/mcp-server-browser-devtools"
      ],
      "env": {}
    }
  }
}